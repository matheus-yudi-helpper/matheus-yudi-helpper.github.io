{
  "manifest_version": 3,
  "version": "0.1.0",
  "name": "MZ Gmail Add-on",
  "description": "MZiQ Extension for Gmail",
  "key": "",
  "icons": {
    "16": "assets/icons/mz/mz-logo-filled.png",
    "32": "assets/icons/mz/mz-logo-filled.png",
    "48": "assets/icons/mz/mz-logo-filled.png",
    "128": "assets/icons/mz/mz-logo-filled.png"
  },
  "oauth2": {
    "client_id": "716875668145-uhkldudo03m4rg68gt32hasmb6i9tn3q.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/gmail.readonly"]
  },
  "permissions": ["identity", "identity.email", "storage"],
  "host_permissions": ["*://inbox.google.com/*", "*://mail.google.com/*"],
  "background": {
    "service_worker": "assets/scripts/background.js"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["*://inbox.google.com/*", "*://mail.google.com/*"],
      "js": ["assets/scripts/injectScripts.js"]
    },
    {
      "run_at": "document_end",
      "matches": ["*://inbox.google.com/*", "*://mail.google.com/*"],
      "js": ["assets/scripts/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://inbox.google.com/*", "*://mail.google.com/*"],
      "resources": ["taskpane.html", "assets/*", "locales/*"]
    }
  ]
}
